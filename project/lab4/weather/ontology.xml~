<?xml version="1.0" encoding="utf-8"?>
<ontology name="WeatherOntology">

  <sort name="title"/>
  <sort name="extent"/>
  <sort name="production"/>

  <predicate name="title_to_search" sort="title"/>
  <predicate name="actors" sort="string"/>
  <predicate name="release" sort="string"/>
  <predicate name="plot" sort="string"/>
  <predicate name="plot_extent" sort="extent"/>
  <predicate name="genre" sort="string"/>
  <predicate name="ratings" sort="string"/>
  <predicate name="production_type" sort="production"/>

  <individual name="title_alien" sort="title"/>
  <individual name="title_cheers" sort="title"/>

  <individual name="extent_full" sort="extent"/>
  <individual name="extent_short" sort="extent"/>
  <individual name="type_movie" sort="production"/>
  <individual name="type_series" sort="production"/>
  <individual name="type_episode" sort="production"/>

</ontology>
